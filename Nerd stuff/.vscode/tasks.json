{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "startreact",
            "command": "npm",
            "args": [
                "start"
            ],
            "type": "shell",
            "problemMatcher": [
                {
                    "pattern": [
                        {
                            "regexp": ".",
